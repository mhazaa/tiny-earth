      <div id="voiceMessageButton"></div>
    </div>

    <div id="itemForms">
      <form v-for="item in items" v-bind:id="item.form.id" class="center" autocomplete="off">
        <div class="exitForm"></div>
        <input v-for="field in item.form.fields" class="inputField" v-bind:class="[(field.isFile) ? 'fileInput' : '']" v-bind:type="field.type" v-bind:name="field.name" v-bind:placeholder="field.placeholder" v-bind:value="field.defaultValue" :required="field.required">
        <input v-if="item.form.id=='audioForm'" type="button" class="button" value="Leave Voice Message">
        <input type="submit" class="submit"  v-bind:class="[(item.form.hasFile) ? 'siofu_submit' : '']" value="upload">
      </form>
    </div>

